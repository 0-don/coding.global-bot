{
  "name": "discord.global-bot",
  "version": "1.0.0",
  "exports": "./dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npx prisma migrate deploy && cross-env NODE_ENV=production node dist/main.js",
    "dev": "nodemon --ignore './**/*.json' --exec node --no-warnings --loader ts-node/esm src/main.ts",
    "prisma": "npx prisma migrate dev --name init && npx prisma generate",
    "prettier": "prettier --ignore-path .gitignore -w ./src",
    "postinstall": "npx prisma generate",
    "build": "tsc",
    "pm2": "npx prisma migrate deploy && cross-env NODE_ENV=production pm2 start dist/main.js --name Lichtblick-GPT4"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.0",
    "@prisma/client": "5.7.0",
    "canvas": "^2.11.2",
    "chart.js": "4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "deepl-node": "1.11.0",
    "discord.js": "14.14.1",
    "discordx": "^11.9.0",
    "js-tiktoken": "^1.0.8",
    "keyv": "^4.5.4",
    "openai": "^4.22.0",
    "quick-lru": "^7.0.0",
    "tesseract.js": "5.0.3"
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1",
    "prettier-plugin-organize-imports": "3.2.4",
    "prisma": "5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}