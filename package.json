{
  "name": "discord.global-bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "prisma migrate deploy && dotenvx run --env NODE_ENV=production -- node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "tsup && tsc --emitDeclarationOnly",
    "prettier": "prettier -w .",
    "prisma": "prisma migrate dev --name init && prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ai-sdk/google": "3.0.13",
    "@discordx/importer": "1.3.3",
    "@elysiajs/cors": "1.4.1",
    "@elysiajs/node": "1.4.3",
    "@elysiajs/openapi": "1.4.14",
    "@prisma/adapter-pg": "7.3.0",
    "@prisma/client": "7.3.0",
    "ai": "6.0.48",
    "dayjs": "1.11.19",
    "deepl-node": "1.24.0",
    "discord.js": "14.25.1",
    "discordx": "11.13.3",
    "elysia": "1.4.22",
    "lru-cache": "11.2.4"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.51.4",
    "@types/bun": "1.3.6",
    "@types/node": "25.0.10",
    "prettier": "3.8.1",
    "prisma": "7.3.0",
    "tsup": "^8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
