    "@elysiajs/cors": "1.4.0",
    "@elysiajs/node": "1.4.2",
    "@elysiajs/openapi": "1.4.11",
    "@prisma/client": "6.19.0",
    "ai": "5.0.106",
    "canvas": "3.2.0",
    "chart.js": "4.5.1",
    "chartjs-adapter-date-fns": "3.0.0",
    "date-fns": "4.1.0",
    "dayjs": "1.11.19",
    "deepl-node": "1.22.0",
    "discord.js": "14.25.1",
    "discordx": "11.13.2",
    "elysia": "1.4.18"
  },
  "devDependencies": {
    "@types/bun": "1.3.3",
    "@types/node": "24.10.1",
    "dotenv": "17.2.3",
    "prettier": "3.7.4",
