{
  "name": "discord.global-bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "dotenvx run --env NODE_ENV=production -- node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "tsup && tsc --emitDeclarationOnly",
    "prettier": "prettier -w .",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:introspect": "drizzle-kit introspect"
  },
  "dependencies": {
    "@ai-sdk/google": "3.0.14",
    "@discordx/importer": "1.3.3",
    "@elysiajs/cors": "1.4.1",
    "@elysiajs/node": "1.4.3",
    "@elysiajs/openapi": "1.4.14",
    "ai": "6.0.53",
    "dayjs": "1.11.19",
    "deepl-node": "1.24.0",
    "discord.js": "14.25.1",
    "discordx": "11.13.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "1.4.22",
    "lru-cache": "11.2.5",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.52.0",
    "@types/bun": "1.3.6",
    "@types/node": "25.0.10",
    "drizzle-kit": "^0.31.8",
    "prettier": "3.8.1",
    "tsup": "^8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
