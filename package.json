{
  "name": "discord.global-bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npx prisma migrate deploy && cross-env NODE_ENV=production node dist/main.js",
    "dev": "nodemon --ignore './**/*.json' --exec node --no-warnings --loader ts-node/esm src/main.ts",
    "prisma": "npx prisma migrate dev --name init && npx prisma generate",
    "prettier": "prettier --ignore-path .gitignore -w ./src",
    "postinstall": "npx prisma generate",
    "build": "tsc"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@hono/node-server": "^1.13.0",
    "@prisma/client": "5.19.1",
    "canvas": "^2.11.2",
    "chart.js": "4.4.4",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "deepl-node": "1.14.0",
    "discord.js": "14.16.2",
    "discordx": "^11.12.1",
    "hono": "^4.6.2",
    "js-tiktoken": "^1.0.14",
    "keyv": "^5.0.1",
    "openai": "^4.62.1",
    "quick-lru": "^7.0.0",
    "tesseract.js": "5.1.1"
  },
  "devDependencies": {
    "@types/node": "22.5.5",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "nodemon": "^3.1.6",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "ts-node": "^10.9.2",
    "typescript": "5.6.2"
  }
}